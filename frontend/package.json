{
  "name": "DocTraining",
  "version": "1.0.0",
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/Theodo-UK/project-generator.git"
  },
  "private": true,
  "engines": {
    "npm": ">=3",
    "node": ">=8.9 <=11.3"
  },
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.config.js --color --progress --hide-modules --display-optimization-bailout",
    "start": "webpack-dev-server --config internals/webpack/webpack.dev.config.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "test:clean": "rimraf ./coverage",
    "jest": "jest --config internals/testing/jest.config.js",
    "test:lint": "eslint --ext js app/",
    "test:jest": "cross-env NODE_ENV=test jest --config internals/testing/jest.config.js",
    "test:e2e": "cross-env NODE_ENV=test jest --config internals/testing/jest.e2e.config.js",
    "test:watch": "yarn test:jest --watchAll",
    "test": "yarn test:lint && yarn flow && yarn test:jest --coverage"
  },
  "pre-commit": "test:lint",
